  "BOT_TOKEN": {
   "description": "YOUR BOT TOKEN GET FROM @botfather",
   "value": ""
  },
  "BOT_USERNAME": {
   "description": "Your bot username",
   "value"
  },
  "HEROKU_API_KEY": {
   "description": "Your Heroku account api key",
   "value": ""
  },
  "HEROKU_APP_NAME": {
   "description": "Your heroku app name",
   "value": ""
  },
  "MAX_SPAM": {
   "description": "No. of pm before approval",
   "value": "3",
   "required": false
  },
  "CMD_HNDLR": {
   "description": "Leave it as it is if you don't know",
   "value": "",
   "required": false
  },
  "PRIVATE_GROUP_ID": {
   "description": "Your private group id it must start with -100....",
   "value": ""
  }
 },
 "addons": [{
  "plan": "heroku-postgresql",
  "options": {
   "version": "12"
  }
 }]
