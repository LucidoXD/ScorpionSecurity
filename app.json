   "description": "Get this value from my.telegram.org!",
   "value": ""
  },
  "API_HASH": {
   "description": "Get this value from my.telegram.org!",
   "value": ""
  },
  "STRING_SESSION": {
   "description": "GET TELETHON string session from https://replit.com/@loverboyXD/SESSIONSTRING-GEN",
   "value": ""
  },  
  "DM_TEXT": {
   "description": "YOUR TEXT WHICH YOU Want to show in DM menu",
