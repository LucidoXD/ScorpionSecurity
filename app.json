  "PM_PIC": {
   "description": "PICTURE that you wana show in PM menu",
   "value": "",
   "required": false
  },
  "OWNER_USERNAME": {
   "description": "Your Username",
   "value": ""
  },
  "OWNER_ID": {
   "description": "Your owner id",
   "value": ""
